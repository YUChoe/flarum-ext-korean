{
    "name": "yongteak/flarum-ext-korean",
    "type": "flarum-extension",
    "description": "Korean language pack.",
    "keywords": ["locale"],
    "license": "MIT",
    "require": {
        "flarum/core": "^0.1.0-beta.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Korean",
            "icon": {
                "image": "star",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "ko",
            "title": "Korean"
        }
    }
}
